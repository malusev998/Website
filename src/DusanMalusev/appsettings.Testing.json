{
  "DetailedErrors": false,
  "ConnectionStrings": {
    "DusanMalusevPostgres": "Host=localhost;Port=5432;Username=postgres;Password=postgres;Timezone=UTC;",
    "DefaultRedis": "localhost:6379,resolveDns=true,allowAdmin=true,connectTimeout=200,connectTimeout=10,keepAlive=10,name=RedisCacheConnection",
    "CacheRedis": "localhost:6379,resolveDns=true,allowAdmin=false,connectTimeout=200,connectTimeout=10,keepAlive=10,name=RedisCacheConnection"
  },
  "Kestrel": {
    "Limits": {
      "MaxConcurrentConnections": 1000,
      "MaxConcurrentUpgradedConnections": 1000,
      "MaxRequestBodySize": 10240,
      "Http2": {
        "HeaderTableSize": 4096,
        "MaxStreamsPerConnection": 100,
        "MaxFrameSize": 16384,
        "MaxRequestHeaderFieldSize": 8192
      }
    },
    "Urls": "https://localhost:5001,http://localhost:5000",
    "DisableStringReuse": false,
    "AllowSynchronousIO": false,
    "AddServerHeader": false
  },
  "Postgres": {
    "Development": false,
    "MinBatchSize": 10,
    "MaxBatchSize": 100
  },
  "Keys": {
    "StoragePath": "Keys",
    "Certificate": {
      "Path": "certificate.pfx",
      "Password": "password123"
    }
  },
  "Sitemap": {
    "Domain": "https://localhost:5001"
  },
  "CsrfCookieOptions": {
    "Name": "XSRF-TOKEN",
    "Domain": "localhost",
    "HeaderName": "X-CSRF-TOKEN",
    "FieldName": "_csrf_field",
    "Path": "/",
    "ExpireIn": 120,
    "Secure": true
  },
  "Google": {
    "ReCaptchaV3": {
      "SecretKey": "",
      "PublicKey": "",
      "UserAgent": "DusanMalusev-WebSite/ReCaptchaV3Client",
      "Url": "https://www.google.com/recaptcha/api/siteverify",
      "HeaderName": "X-ReCaptchaV3-Token",
      "Threshold": 0.5
    },
    "Analytics": ""
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "Microsoft": "Error",
        "System": "Error",
        "Microsoft.AspNetCore.DataProtection": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "encoding": "System.Text.Encoding::UTF8",
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
        }
      },
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "Logs/malusev.jsonl",
                "rollingInterval": "Day",
                "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
                "encoding": "System.Text.Encoding::UTF8"
              }
            }
          ]
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithEnvironmentUserName",
      "WithMachineName",
      "WithProcessName",
      "WithProcessId",
      "WithThreadName",
      "WithThreadId",
      "WithMemoryUsage"
    ],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": {
          "maximumDestructuringDepth": 4
        }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": {
          "maximumStringLength": 100
        }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": {
          "maximumCollectionCount": 10
        }
      }
    ],
    "Properties": {
      "Application": "DusanMalusev"
    }
  }
}
